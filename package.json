{
    "name": "rune-helper",
    "version": "0.2.5",
    "scripts": {
        "ng": "ng",
        "start": "ng serve --port 666",
        "start_prod": "ng serve --port 666 --configuration production",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test --source-map=false --watch=false --code-coverage",
        "stats": "npm run stats-build && npm run stats-display",
        "stats-build": "ng build --stats-json",
        "stats-display": "webpack-bundle-analyzer ./dist/Diablo2Helper/stats.json",
        "patch": "npx patch-package @dschu012/d2s",
        "postinstall": "patch-package",
        "preversion": "npm test --source-map=false --watch=false --code-coverage",
        "postversion": "git flow release finish -n"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~13.0.0",
        "@angular/common": "~13.0.0",
        "@angular/compiler": "~13.0.0",
        "@angular/core": "~13.0.0",
        "@angular/forms": "~13.0.0",
        "@angular/platform-browser": "~13.0.0",
        "@angular/platform-browser-dynamic": "~13.0.0",
        "@angular/router": "~13.0.0",
        "@dschu012/d2s": "^2.0.4",
        "@fortawesome/angular-fontawesome": "^0.10.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.36",
        "@fortawesome/free-brands-svg-icons": "^5.15.4",
        "@fortawesome/free-regular-svg-icons": "^5.15.4",
        "@fortawesome/free-solid-svg-icons": "^5.15.4",
        "@ng-web-apis/common": "^1.12.1",
        "@ng-web-apis/resize-observer": "^1.0.3",
        "@ngx-translate/core": "^14.0.0",
        "@ngx-translate/http-loader": "^7.0.0",
        "@tinkoff/ng-event-plugins": "^2.3.2",
        "patch-package": "^6.4.7",
        "rxjs": "~7.4.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~13.0.1",
        "@angular/cli": "~13.0.1",
        "@angular/compiler-cli": "~13.0.0",
        "@types/jasmine": "~3.10.2",
        "@types/node": "^16.11.6",
        "@types/resize-observer-browser": "^0.1.6",
        "jasmine-core": "^3.10.1",
        "karma": "^6.3.8",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.0.3",
        "karma-jasmine": "~4.0.1",
        "karma-jasmine-html-reporter": "~1.7.0",
        "prettier": "^2.4.1",
        "typescript": "~4.4.4"
    }
}
